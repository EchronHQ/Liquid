<?php
declare(strict_types=1);

use Liquid\Content\Block\HtmlHeadBlock;
use Liquid\Content\Block\TemplateBlock;

assert($this instanceof TemplateBlock);

$headBlock = $this->getLayout()->getBlock('head');
if ($headBlock instanceof HtmlHeadBlock) {
    $scripts = $headBlock->getScripts();

    foreach ($scripts as $script) {
        echo $headBlock->renderScript($script);
    }
}
