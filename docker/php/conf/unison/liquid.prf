# Roots of the synchronization
root = /home/liquid
root = /var/www

# Paths to synchronize
path = liquid
path = liquid

# Params
fastercheckUNSAFE = true
ignorelocks = true
numericids = false
log = true
batch = true
fastcheck = true
confirmbigdel = false
ignore = Path */.git
ignore = Path */.idea
# ignore = Path */update/vendor/composer/composer/tests/Composer/Test/Autoload/Fixtures/classmap/LargeClass.php
prefer = /home/liquid
